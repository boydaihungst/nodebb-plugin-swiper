  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.18.6"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-angular": "15.0.0",
    "eslint": "8.2.0",
    "eslint-config-nodebb": "0.1.1",
    "eslint-plugin-import": "2.25.3",
    "husky": "7.0.4",
    "lint-staged": "12.0.3"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "swiper": "^7.3.0",
    "wiper": "^1.3.0"
