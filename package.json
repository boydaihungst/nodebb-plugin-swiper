{
  "name": "nodebb-plugin-swiper",
  "version": "1.0.1",
  "description": "Add images gallery to nodebb",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/boydaihungst/nodebb-plugin-swiper"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "swiper",
    "shell"
  ],
  "husky": {
